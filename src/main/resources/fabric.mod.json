{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",

  "name": "${name}",
  "description": "Template for my fabric mods",
  "authors": ["a3510377"],
  "contact": {
    "homepage": "https://modrinth.com",
    "sources": "https://github.com/a3510377/fabric-mod-template"
  },

  "icon": "assets/template_mod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": ["monkey.template_mod.TemplateMod"]
  },
  "mixins": [],

  "depends": {
    "fabricloader": ">=0.11.4",
    "minecraft": "${minecraft_dependency}"
  },

  "custom": {
    "mc-publish": {
      "modrinth": "${modrinth_project_id}",
      "curseforge": "${curseforge_project_id}"
    }
  }
}
